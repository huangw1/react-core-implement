{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-underscore-dangle":"off",
        "no-use-before-define":"off",
        "no-param-reassign":"off",
        "new-cap":"off",
        "no-nested-ternary":"off",
        "no-plusplus":"off",
        "no-continue":"off"
    }
}
